{
  "name": "grpc-rocket",
  "displayName": "gRPC rocket",
  "description": "nan",
  "version": "0.0.1",
  "icon": "images/rocket.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/Dancheg97/grpc_rocket"
  },
  "engines": {
    "vscode": "^1.66.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:grpc-rocket.helloWorld",
    "onCommand:grpc-rocket-sidebar.reloadSidebar"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "grpc-rocket.helloWorld",
        "title": "Hello World"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "grpc-rocket-sidebar-view",
          "title": "gRPC rocket",
          "icon": "images/rocket.svg"
        }
      ]
    },
    "views": {
      "grpc-rocket-sidebar-view": [
        {
          "type": "webview",
          "id": "grpc-rocket-sidebar",
          "name": "gRPC rocket",
          "icon": "images/rocket.svg",
          "contextualTitle": "gRPC rocket"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.66.0",
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.0",
    "@types/node": "14.x",
    "@typescript-eslint/eslint-plugin": "^5.12.1",
    "@typescript-eslint/parser": "^5.12.1",
    "eslint": "^8.9.0",
    "glob": "^7.2.0",
    "mocha": "^9.2.1",
    "typescript": "^4.5.5",
    "ts-loader": "^9.2.6",
    "webpack": "^5.69.1",
    "webpack-cli": "^4.9.2",
    "@vscode/test-electron": "^2.1.2"
  }
}
